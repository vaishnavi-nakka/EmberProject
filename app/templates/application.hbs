
<div class="container">

  <div class="panel panel-default">
  <div class="panel-heading" id="order">Order-Search</div>
  <div class="panel-body">
     <div class="row">
      <div class="col-sm-12" >
    <label >OMX#</label>
        {{
        input
        type="text"
        class="omx-id"
        placeholder="omxId"
        value=omxSearch
        }}
       
        
        <label >GPS#</label>
        {{
        input
        type="text"
        id="gps-id"
        placeholder="GPS HostName"
        value=gpsSearch
        }}
        
  <button type="button" class="btn" {{action "onSubmit" omxSearch gpsSearch}}>Search</button>
  </div>
       </div>
  </div>
</div>





{{#bs-accordion as |acc|}}

  {{#acc.item value="1" as |aitem|}}
  <div  class="accord-title" {{action "toggleAccordian"}}>
